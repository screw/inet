<?xml version="1.0"?>
<OSPFASConfig xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="OSPF.xsd">

  <!-- Areas -->
  <Area id="0.0.0.0">
   
   <AddressRange address="routerCore1>routerCore2" mask="routerCore1>routerCore2" status="DoNotAdvertise" />
    <AddressRange address="routerCore1>routerCore5" mask="routerCore1>routerCore5" status="DoNotAdvertise" />
    <AddressRange address="routerCore3>routerCore4" mask="routerCore3>routerCore4" status="DoNotAdvertise" />
    <AddressRange address="routerCore3>routerCore5" mask="routerCore3>routerCore5" status="DoNotAdvertise" />
    <AddressRange address="routerCore5>routerCore7" mask="routerCore5>routerCore7" status="DoNotAdvertise" />
    <AddressRange address="routerCore5>routerCore8" mask="routerCore5>routerCore8" status="DoNotAdvertise" />
    <AddressRange address="routerCore6>routerCore7" mask="routerCore6>routerCore7" status="DoNotAdvertise" />
    <AddressRange address="routerCore6>routerCore8" mask="routerCore6>routerCore8" status="DoNotAdvertise" />

    <AddressRange address="routerCore2>routerCore6" mask="routerCore2>routerCore6" status="DoNotAdvertise" />
    <AddressRange address="routerCore4>routerCore6" mask="routerCore4>routerCore6" status="DoNotAdvertise" />
	 
	<AddressRange address="sender1" mask="sender1" status="Advertise" />
    <AddressRange address="receiver1" mask="receiver1" status="Advertise" />
    <AddressRange address="sender2" mask="sender2" status="Advertise" />
    <AddressRange address="receiver2" mask="receiver2" status="Advertise" />
    
	 
	 
    
    <AddressRange address="sender1>routerCore1" mask="sender1>routerCore1" status="Advertise" />
    <AddressRange address="receiver1>routerCore2" mask="receiver1>routerCore2" status="Advertise" />
    <AddressRange address="sender2>routerCore3" mask="sender2>routerCore3" status="Advertise" />
    <AddressRange address="receiver2>routerCore4" mask="receiver2>routerCore4" status="Advertise" />
  

    
  </Area>

  <!-- Routers -->
  <Router name="routerCore1" RFC1583Compatible="true">
    <BroadcastInterface ifName="eth0" areaID="0.0.0.0" interfaceOutputCost="1" />
    <PointToPointInterface ifName="eth1" areaID="0.0.0.0" interfaceOutputCost="4" />
    <PointToPointInterface ifName="eth2" areaID="0.0.0.0" interfaceOutputCost="1" />
 
  </Router>
  
  <Router name="routerCore2" RFC1583Compatible="true">
    <BroadcastInterface ifName="eth0" areaID="0.0.0.0" interfaceOutputCost="1" />
    <PointToPointInterface ifName="eth1" areaID="0.0.0.0" interfaceOutputCost="4" />
    <PointToPointInterface ifName="eth2" areaID="0.0.0.0" interfaceOutputCost="1" />
 
  </Router>
  
  <Router name="routerCore3" RFC1583Compatible="true">
    <BroadcastInterface ifName="eth0" areaID="0.0.0.0" interfaceOutputCost="1" />
    <PointToPointInterface ifName="eth1" areaID="0.0.0.0" interfaceOutputCost="1" />
    <PointToPointInterface ifName="eth2" areaID="0.0.0.0" interfaceOutputCost="4" />
  </Router>
  
  <Router name="routerCore4" RFC1583Compatible="true">
    <BroadcastInterface ifName="eth0" areaID="0.0.0.0" interfaceOutputCost="1" />
    <PointToPointInterface ifName="eth1" areaID="0.0.0.0" interfaceOutputCost="1" />
    <PointToPointInterface ifName="eth2" areaID="0.0.0.0" interfaceOutputCost="4" />
  </Router>
  
  
  <Router name="routerCore5" RFC1583Compatible="true">
    <PointToPointInterface ifName="eth0" areaID="0.0.0.0" interfaceOutputCost="1" />
    <PointToPointInterface ifName="eth1" areaID="0.0.0.0" interfaceOutputCost="1" />
    <PointToPointInterface ifName="eth2" areaID="0.0.0.0" interfaceOutputCost="1" />
    <PointToPointInterface ifName="eth3" areaID="0.0.0.0" interfaceOutputCost="1" />
  </Router>
  
  <Router name="routerCore6" RFC1583Compatible="true">
    <PointToPointInterface ifName="eth0" areaID="0.0.0.0" interfaceOutputCost="1" />
    <PointToPointInterface ifName="eth1" areaID="0.0.0.0" interfaceOutputCost="1" />
    <PointToPointInterface ifName="eth2" areaID="0.0.0.0" interfaceOutputCost="1" />
    <PointToPointInterface ifName="eth3" areaID="0.0.0.0" interfaceOutputCost="1" />
 
  </Router>
  
  <Router name="routerCore7" RFC1583Compatible="true">
    <PointToPointInterface ifName="eth0" areaID="0.0.0.0" interfaceOutputCost="1" />
    <PointToPointInterface ifName="eth1" areaID="0.0.0.0" interfaceOutputCost="1" /> 
  </Router>
 
  <Router name="routerCore8" RFC1583Compatible="true">
    <PointToPointInterface ifName="eth0" areaID="0.0.0.0" interfaceOutputCost="1" />
    <PointToPointInterface ifName="eth1" areaID="0.0.0.0" interfaceOutputCost="1" /> 
  </Router>
  


</OSPFASConfig>